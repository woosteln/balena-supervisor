    "strict-event-emitter-types": "^2.0.0",
    "terser": "^3.14.1",
    "tmp": "^0.1.0",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.6.2",
    "typed-error": "^2.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "winston": "^3.2.1",
    "yargs": "^15.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
